<% content_for :head do %>
  <%= wicked_pdf_stylesheet_link_tag "application-print" %>
<% end %>

<% cache @calendar do %>
  <% @pages_count.times do |page_offset| %>
    <header class="heading">
      <%= render :partial => 'heading', locals: {f: nil, page_offset: page_offset} %>
    </header>

    <%= render :partial => 'calendar_table', locals: {f: nil, page_offset: page_offset} %>

    <%= render :partial => 'footing' %>
  <% end %>
<% end %>

